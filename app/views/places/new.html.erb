<h2> Add A Study Spot?</h2>
<%= form_for @place do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>
	<%= f.label :description %><br>
	<%= f.text_area :description %><br>
	<%= f.select :category_id, Category.all.map{ |f| [f.name, f.id] } %><br>
	<%= f.label :address1 %><br>
	<%= f.text_field :address1%><br>
	<%= f.label :address2 %><br>
	<%= f.text_field :address2 %><br>
	<%= f.label :city %><br>
	<%= f.text_field :city %><br>
	<%= f.label :state %><br>
	<%= f.text_field :state %><br>
	<%= f.label :zipcode %><br>
	<%= f.text_field :zipcode %><br>
	<%= f.label :phone %><br>
	<%= f.text_field :phone %><br>
	<%= f.label :email %><br>
	<%= f.text_field :email %><br>
	<%= f.submit %><br>
<% end %>	

